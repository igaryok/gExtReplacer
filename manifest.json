{
  "manifest_version": 2,

  "name": "Replacer Plugin",
  "description": "This extension will replace words in input",
  "version": "1.0",

  "icons": {
    "128": "icon.png"
  },  
  
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": ["replaceInput.js", "replaceContenteditable.js"],
      "run_at":"document_end"
    }
  ]
}